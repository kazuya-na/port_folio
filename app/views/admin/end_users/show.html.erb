<div class="container p-5">
  <div class="row">
    <div class="col-12">
      <h1><%= @end_user.nick_name %>さんのプロフィール</h1>
      <%= link_to '退会はこちら', withdraw_confirm_path, class: 'btn btn-danger' %>
      <%= link_to '編集', edit_admin_end_user_path(@end_user), class: 'btn btn-success' %>
      <%= link_to '日誌', diaries_path, class: 'btn btn-info' %>
    </div>
    <div class="col">
      <%= render 'info', end_user: @end_user %>
    </div>
  </div>
</div>
